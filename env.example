# Configurações do Banco de Dados
DB_HOST=localhost
DB_PORT=5432
DB_NAME=precivox
DB_USER=postgres
DB_PASSWORD=sua_senha_aqui
DATABASE_URL=postgresql://postgres:sua_senha_aqui@localhost:5432/precivox

# Configurações do Servidor
NODE_ENV=development
PORT=3001
JWT_SECRET=seu_jwt_secret_aqui

# URLs de API
# Em desenvolvimento: deixe vazio ou use http://localhost:3001
# Em produção: use https://precivox.com.br
NEXT_PUBLIC_API_URL=
API_URL=

# APIs Externas
GROQ_API_KEY=sua_groq_api_key_aqui
OPENCAGE_API_KEY=sua_opencage_api_key_aqui

# Configurações de Segurança
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=5

# Configurações de Upload
MAX_FILE_SIZE=52428800
UPLOAD_PATH=./uploads

# Next.js
NEXT_PUBLIC_NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=seu_nextauth_secret_aqui
NEXTAUTH_URL=http://localhost:3000
